    private void jMenuItem3ActionPerformed(java.awt.event.ActionEvent evt) {                                           
   int dialogResult = JOptionPane.showConfirmDialog (this, "Are you sure you want to log out?","Warning",JOptionPane.YES_NO_OPTION);
            if(dialogResult == JOptionPane.YES_OPTION){
                 System.exit(0);
            } 

    }                                          

    private void jLabel5MousePressed(java.awt.event.MouseEvent evt) {                                     
showappo x = new showappo();
if(x.isAppo()==false){
     NewJFrame8 frameba = new  NewJFrame8();
                  frameba.setVisible(true);       
    
}
if(x.isAppo()==true){
      NewJFrame13 see = new NewJFrame13();
    see.setVisible(true);
}
       
    }                                    

